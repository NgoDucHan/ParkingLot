<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="view_parkinglots_summary_wizard" model="ir.ui.view">
            <field name="name">parkinglot.report.wizard.form</field>
            <field name="model">parkinglot.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Time in Parking Lot">
                    <group col="4" colspan="6">
                        <field name="date_from"/>
                        <newline/>
                        <field name="date_to"/>
                        <newline/>
                        <field name="parkinglots_id" invisible="True"/>
                    </group>
                    <footer>
                        <button name="print_report" string="Print" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_parkinglots_summary_wizard" model="ir.actions.act_window">
            <field name="name">Parking Lot Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">parkinglot.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="binding_model_id" ref="model_parkinglot_parkinglots" />
            <field name="binding_type">report</field>
        </record>

</odoo>